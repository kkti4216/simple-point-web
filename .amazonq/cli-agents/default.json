{
  "name": "default",
  "description": "Default agent",
  "tools": ["*"],
  "allowedTools": [
    "fs_read",
    "@awslabs.core-mcp-server",
    "@aws-knowledge-mcp-server",
    "@awslabs.aws-pricing-mcp-server",
    "@awslabs.ccapi-mcp-server/create_template",
    "@awslabs.ccapi-mcp-server/explain",
    "@awslabs.ccapi-mcp-server/get_aws_account_info",
    "@awslabs.ccapi-mcp-server/get_aws_session_info",
    "@awslabs.ccapi-mcp-server/get_resource",
    "@awslabs.ccapi-mcp-server/get_resource_request_status",
    "@awslabs.ccapi-mcp-server/get_resource_schema_information",
    "@awslabs.ccapi-mcp-server/list_resources",
    "@awslabs.ccapi-mcp-server/run_checkov",
    "@awslabs.cost-explorer-mcp-server"
  ],
  "resources": [
    "file://AmazonQ.md",
    "file://README.md", 
    "file://.amazonq/rules/**/*.md"
  ],
  "mcpServers": {
    "awslabs.core-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.core-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false
    },
    "aws-knowledge-mcp-server": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://knowledge-mcp.global.api.aws"
      ],
      "disabled": false
    },
    "awslabs.ccapi-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.ccapi-mcp-server@latest"
      ],
      "disabled": false
    },
    "awslabs.aws-pricing-mcp-server": {
      "command": "uvx",
      "args": [
        "awslabs.aws-pricing-mcp-server@latest"
      ],
      "env": {
        "FASTMCP_LOG_LEVEL": "ERROR"
      },
      "disabled": false
    },
    "awslabs.cost-explorer-mcp-server": {
        "command": "uvx",
        "args": [
            "awslabs.cost-explorer-mcp-server@latest"
        ],
        "env": {
            "FASTMCP_LOG_LEVEL": "ERROR",
            "AWS_PROFILE": "your-aws-profile"
        },
        "disabled": false
    }
  },
  "useLegacyMcpJson": false
}